{
  "name": "@project-x/api-gateway-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@project-x/api-gateway"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/api-gateway-e2e/jest.config.cts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@project-x/api-gateway:build",
          "@project-x/api-gateway:serve"
        ]
      }
    }
  }
}
